{
  "name": "nef-appstore",
  "version": "1.0.0",
  "description": "CTA download nef Playgrounds from App Store",
  "main": "dist/bundle.js",
  "author": "nef",
  "license": "MIT",
  "keywords": [
    "nef",
    "bow",
    "Xcode Playground",
    "nef Playground",
    "Swift Playground",
    "Playgrounds",
    "iPad"
  ],
  "scripts": {
    "build": "npm run clean; npm run ncc; npm run browserify",
    "ncc": "ncc build ./src/main.ts --minify",
    "browserify": "browserify ./dist/index.js -o ./dist/bundle.js; rm -rf ./dist/index.js; cp ./src/*html ./dist; cp ./src/*css ./dist",
    "clean": "rm -rf dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^14.0.14",
    "typescript": "^3.9.6",
    "@zeit/ncc": "^0.22.0",
    "browserify": "^16.5.1"
  }
}
